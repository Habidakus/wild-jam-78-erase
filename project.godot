[display]

window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/minmaxcalculator/plugin.cfg", "res://addons/statemachine/plugin.cfg")

[gui]

theme/custom_font="res://Font/LibertinusSerif-Regular.otf"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
